{
  "name": "template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vitest run && turbo vite.build",
    "build-storybook": "storybook build",
    "coverage": "vitest run --coverage",
    "dev": "turbo vite.dev",
    "lint": "tsc",
    "prepare": "is-ci || husky install",
    "preview": "turbo vite.preview",
    "storybook": "storybook dev -p 6006",
