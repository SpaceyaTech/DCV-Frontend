    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.1.8",
    "hugeicons-react": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.26.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tilg": "0.1.1",
    "vaul": "^0.9.1"
