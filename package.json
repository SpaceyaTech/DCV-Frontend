    "lightningcss": "1.26.0",
    "lint-staged": "15.2.8",
    "postcss": "8.4.41",
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.1",
    "prettier-plugin-tailwindcss": "0.6.5",
    "sort-package-json": "2.10.0",
    "storybook": "^8.2.8",
    "tailwindcss": "3.4.7",
    "turbo": "2.0.12",
    "typescript": "5.5.4",
    "vite": "5.3.5",
    "vite-plugin-mkcert": "1.17.5",
    "vite-tsconfig-paths": "4.3.2",
